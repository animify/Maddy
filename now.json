{
    "version": 2, 
    "public": true,
    "name": "Reel",
    "builds": [
        {
            "src": "server/index.js",
            "use": "@now/node-server"
        },
        {
            "src": "next.config.js",
            "use": "@now/next"
        }
    ],
    "routes": [
        {
            "src": "/hooks/(.*)",
            "dest": "/server/index.js",
            "methods": ["GET", "POST"]
        }
    ]
  }